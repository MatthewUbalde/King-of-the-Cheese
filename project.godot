;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="King of the Cheese"
config/description="Watch as many cheeses grow around Dark Birthday each day in this fan-game of King of the Hat! RUN, EAT, and take PICTURES! As you go around in this very small and limited confined space eating cheese."
run/main_scene="res://scenes/user_interface/splash_screen_transition.tscn"
config/features=PackedStringArray("4.1", "Mobile")
run/max_fps=360
boot_splash/bg_color=Color(0.905882, 0.666667, 0.211765, 1)
boot_splash/show_image=false
boot_splash/image="res://assets/splash_screen/Made in Godot Splash Screen.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://Cheese_logo.png"
config/windows_native_icon="res://Cheese_logo.ico"
boot_splash/minimum_display_time=10

[audio]

buses/default_bus_layout="res://assets/audio_bus/default_bus_layout.tres"

[autoload]

GameEvents="*res://classes/autoloads/game_events.tscn"
Ultilities="*res://classes/autoloads/ultilities.tscn"
SecretMessage="*res://classes/autoloads/secret_message.tscn"
ScenePackages="*res://classes/autoloads/scene_packages.tscn"
InsanityEvents="*res://scenes/insanity/insanity_events.tscn"
DiscordSDKLoader="*res://addons/discord-sdk-gd/nodes/core_updater.gd"
ScoreManager="*res://classes/autoloads/score_manager.tscn"
SoundResources="*res://classes/autoloads/SoundResources.tscn"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/mode=3
window/size/initial_position_type=3
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://assets/gui/mouse_pointer.png"

[editor]

movie_writer/movie_file="C:/Users/matth/Videos/GodotVideos/king_of_the_cheese.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/discord-sdk-gd/plugin.cfg")

